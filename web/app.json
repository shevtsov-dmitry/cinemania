{
    "expo": {
        "name": "cinemania",
        "slug": "cinemania",
        "version": "0.0.1",
        "orientation": "portrait",
        "icon": "./images/icons/company_logo.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./images/icons/company_logo.png",
            "resizeMode": "contain",
            "backgroundColor": "#FEF9B0"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./images/icons/company_logo.png",
                "backgroundColor": "#ffffff"
            }
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./images/icons/company_logo.png"
        },
        "plugins": ["expo-router"],
        "experiments": {
            "typedRoutes": true
        }
    }
}
